{
	"name": "MSPAuthoriser",
	"author": "Harald Warmelink & Dragomir Mitev",
	"url": "",
	"description": "This extension makes your wiki use the https://auth.mspchallenge.info API for logging users in and redirecting users for account registration. It also makes MediaWiki use the Gmail API to send e-mails.",
	"version": "1.0",
	"license-name": "GPL-2.0-or-later",
	"type": "other",
	"manifest_version": 2,
 	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\MSPAuthoriser\\": "includes/"
	},
	"AutoloadClasses": {
		"SpecialGmailAPIPermission": "src/Special.php"
	},
  	"MessagesDirs": {
		"MSPAuthoriser": [
			"i18n"
		]
	},
	"Hooks": {
		"PersonalUrls": "MediaWiki\\Extension\\MSPAuthoriser\\Hooks::onPersonalUrls",
		"AlternateUserMailer": "MediaWiki\\Extension\\MSPAuthoriser\\Hooks::onAlternateUserMailer"
	},
	"config": {
		"PluggableAuth_Class": {
			"value": "MediaWiki\\Extension\\MSPAuthoriser\\PluggableAuth"
		}
	},
	"ResourceModules": {
		"x.MSPAuthoriser.styles": {
			"styles": [
				"x.mystyles.css"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "MSPAuthoriser"
	},
	"SpecialPages": {
		"GmailAPIPermission": "SpecialGmailAPIPermission"
	}
}
